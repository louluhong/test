<!--   UA_Opt初始化配置(必须在uab.js加载之前)、uab.js引入必须在页面的foot段加载 -->
<script type="text/javascript">
    if(!window.UA_Opt){
            var UA_Opt = new Object;
            UA_Opt.SendMethod = 9; // 9保存到指定表单和指定js变量，1只保存到表单，8只保存到js变量中
            UA_Opt.FormId = "login_submit_form"; //SendMethod=8时（保存至js变量中）不用指定；保存ua到表单时，必须指定
            UA_Opt.ExTarget = ['password']; //可选，不需要记录事件的元素id
            UA_Opt.LogVal = "ua"; //SendMethod=1时（保存至表单中）不用指定；保存ua到变量时，必须指定;在浏览器console里面可查询
            window[UA_Opt.LogVal] = ""; //变量初始化内容清空
            UA_Opt.Token = new Date().getTime() + ":" + Math.random();
            //(>_<)对发送数据长度有限制的系统，注意下面的配置，
            //一般最大采集个数设置为50（??调用notify接口），
            //对长度有特殊要求的可设置为10（??GET方式提交）
            UA_Opt.MaxMCLog = 50; //鼠标事件最大获取个数，可选，默认150
            UA_Opt.MaxKSLog = 50; //键盘事件最大获取个数，可选，默认150
            UA_Opt.MaxMPLog = 50; //鼠标移动事件最大获取个数，可选，默认150
            UA_Opt.Sync = true;  //表示用于同步加载
            //UA_Opt.MaxGPLog = 5; //默认选择5，ua中记录陀螺仪事件的最??大个数。 
            UA_Opt.MaxTCLog = 50; //默认选择150，ua中记录WAP页面touch事件的最??大个数。
            UA_Opt.MaxFocusLog = 50;//默认选择150，ua中记录焦点切换事件的最??大个数。
            //UA_Opt.isSendError = 1; // 报告执??异常，默认值为1；??线应??配置值0
            UA_Opt.Flag = 1670350; //PC，无线通用。需要捕获陀螺仪事件用1932494
    }
 
            function getUA () { 
                //调用风控接口，前一刻，再去取值
                //（collina值会因为用户在页面上的动作不断的增加，等到用户完成业务动作，调用风控接口时，再取出collina值发送）
        var tmp_ua = window[UA_Opt.LogVal];
            UA_Opt.Token= new Date().getTime()+":"+Math.random(); 
            UA_Opt.reload && UA_Opt.reload(); 
        return tmp_ua;
        }
</script>
<script src="https://acjs.aliyun.com/js/uab.js"></script>