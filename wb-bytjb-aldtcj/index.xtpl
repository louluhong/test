<div class="module-wrap J_tb_lazyload">
	<textarea class="J_conf dataText" style="display:none;">{{$env.JSON.stringify(this)}}</textarea>
	<div class="by_con" style="background: {{moduleinfo.mod_bgcolor}} ">
		<div class="by_w990">
			<div class="by_tm {{#if(moduleinfo.da_num==='3')}}by_three{{/if}} {{#if(moduleinfo.da_num==='2' && moduleinfo.pd_more===false||moduleinfo.pd_more==='false'||moduleinfo.pd_more==='否')}}by_two{{/if}} {{#if(moduleinfo.da_num==='4')}}by_three{{/if}}"style="background:{{moduleinfo.dt_bgcolor}};">
				<img src="{{moduleinfo.dt_icon}}" alt="" class="tm_icon" style="bottom: 2px;left: 5px;">
				<span class="tm_title" style="color:{{moduleinfo.dt_color}};">{{moduleinfo.title}}</span>
				<ul class="da_ul" data-acid="{{moduleinfo.myacid}}" data-asac="{{moduleinfo.myasac}}">
					{{#each(items)}}
					<li class="da_li" style="background: {{moduleinfo.dt_btn}};{{#if(moduleinfo.pd_more===true||moduleinfo.pd_more==='true'||moduleinfo.pd_more==='是')}}border:2px solid {{moduleinfo.more_bg}};{{/if}}">
						<a href="javascript:void(0);" target="_self" style="color:{{moduleinfo.da_color}};" data-id="{{moduleinfo.dt_id}}" class="ans-btn {{#if(yes_ask===true||yes_ask==='true'||yes_ask==='是')}}ask-right{{/if}} {{#if(moduleinfo.pd_hdtime===true||moduleinfo.pd_hdtime==='true')}}over{{/if}}">{{da}}</a>
					</li>
					{{/each}}
				</ul>
				<a href="{{moduleinfo.more_link}}" class="by_more iconfont" target="_self" style="background:{{moduleinfo.more_bg}};color:{{moduleinfo.dt_color}};"><b>查看更多</b>{{#if(moduleinfo.pd_more===true||moduleinfo.pd_more==='true'||moduleinfo.pd_more==='是')}}<i>&#xe600;</i>{{/if}}</a>
			</div>
		</div>
		<div class="toast_tk"></div> 
	</div>
